function changeCorpAddrCity() {
    var corpAddrCityCodeVal = $("#corpAddrCityCode").val();
    var corpAddrCityCodeText = $("#corpAddrCityCode").find("option:selected").text();
    if (corpAddrCityCodeVal != "") {
        $("#corpAddrCity").val(corpAddrCityCodeText);
    }
};
function changeCorpAddrProv() {
    var corpAddrProvCodeVal = $("#corpAddrProvCode").val();
    var corpAddrProvCodeText = $("#corpAddrProvCode").find("option:selected").text();
    if (corpAddrProvCodeVal != "") {
        $("#corpAddrProv").val(corpAddrProvCodeText);
        //设置city值
        //查询省份对应的城市，然后显示到页面
        var cityArray = getProvinceAndCity();
        var cityOption = cityArray[corpAddrProvCodeVal];
        $("#corpAddrCityCode").html(cityOption);
        $("#corpAddrCity").val("");
    } else {
        $("#corpAddrCityCode").html("<option value=''>--请选择--</option>");
        $("#corpAddrCity").val("");
    }
};
function addNewRefIdContent() {
    var refIdContentsSize = $("#refIdContentsSize").val();
    $("#refIdContentsSize").val(parseInt(refIdContentsSize) + 1);
    //$("#refIdContentTr"+(parseInt(refIdContentsSize)-1)).after("<dl id='refIdContentTr"+refIdContentsSize+"'><dt>关联快钱账户：</dt><dd><input type='text' id='refIdcontent"+refIdContentsSize+"' name='refIdcontent"+refIdContentsSize+"' class='form-text' value=''  onblur=\"javascript:checkRefIdContent('"+refIdContentsSize+"');\"/></dd><dd><span id='refIdcontent"+refIdContentsSize+"Error'></span></dd></dl>");
    $("#refIdContentTr" + (parseInt(refIdContentsSize) - 1)).after("<dl class='info-item' id='refIdContentTr" + refIdContentsSize + "'><dt class='key'>关联快钱账户：</dt><dd class='val'><input type='text' id='refIdcontent" + refIdContentsSize + "' name='refIdcontent" + refIdContentsSize + "' class='form-text' value=''  onblur=\"javascript:checkRefIdContent('" + refIdContentsSize + "');\"/></dd><dd><span id='refIdcontent" + refIdContentsSize + "Error'></span></dd></dl>");

};
function changeIdContentName(selectIdBill) {
    if (selectIdBill == "0") {
        $("#idContentTip").html("快钱账户：");
        $("#corpName").attr("readonly", true);
    } else {
        $("#idContentTip").html("注册快钱账户：");
        $("#corpName").attr("readonly", false);
    }
    var idContent = $("#idContent").val();
    idContent = $.trim(idContent);
    if (idContent != "") {
        checkIdContent();
    }
};
function to_getRadio() {
    var isBillValue
    var isBills = document.getElementsByName("isBill");
    for (var i = 0; i < isBills.length; i++) {
        if (isBills[i].checked == true) {
            isBillValue = isBills[i].value;
        }
    }
    return isBillValue;
};

var checkResultFlag = false;
function productCheckById() {
    var productCode = '1';
    var idContent = $("#idContent").val();
    var refIdContentsSize = $("#refIdContentsSize").val();
    var refIdContents = "";
    for (var i = 0; i < refIdContentsSize; i++) {
        if ($("#refIdcontent" + i).val().length != 0) {
            refIdContents = refIdContents + $.trim($("#refIdcontent" + i).val()) + ",";
        }
    }

    $("#popup_loanding_id").show();
    var url = "/ifs-fsp-fe/checkProductByCrs?productId=" + productCode + "&idContent=" + idContent + "&refIdContents=" + refIdContents + "&_time=" + new Date();
    url = encodeURI(url);
    $.ajax({
        type: "get",
        url: url,
        //data:{productId:productCode,idContent:idContent},
        cache: false,
        async: false,
        dataType: 'json',
        success: function(data) {
            var flag = data.flag;
            $("#popup_loanding_id").hide();
            if (flag == '-1') { //不通过
                $("#popup_result_id").show();
                $("#resultMsg").html("你的流水不满足要求，无法申请");
                $("#checkResultId").addClass("fali");
            } else {
                checkResultFlag = true;
            }
        },
        error: function(data) {
            $("#popup_loanding_id").hide();
            alert("系统错误，请联系运维人员");
        }
    });
    return checkResultFlag;
}

//关闭弹出窗口
function closePopupsWindows(data) {
    var $popup = $("#" + data);
    $popup.hide();
};
</script>
