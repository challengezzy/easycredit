 	function callback(retcode,retMsg,excelName){
 		//TODO 待优化
  		alert(retMsg);
  		$("#uploadFlowError").html("");
  		$("#uploadFlowError").removeClass("err");
  		if("000000"==retcode){
  			//rzt显示当前上传成功的原文件名称
  			$("#flowExcelName").html("");
