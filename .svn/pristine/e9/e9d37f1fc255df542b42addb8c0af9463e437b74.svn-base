 	//提示确认对话框
 	function agreeConfirm(idLoanOrder,payFlowKind){
 		//流水期限必须选择
 		if(!checkPayFlowKind()){
 			return false;
 		}
 		//营业执照商户名称校验
 		if(!checkEmpty("regiCorpName")){
 			return false;
 		}
 		var payFlowKind=$("#payFlowKind").val();	 		
 		var amountRMB=payFlowKind=="6"?"35":"65";
 		var flowDay=payFlowKind=$("#payFlowKind").val();
	
		//如果购买过程尚未结束，则不提示confirm
		if("-99999"==payResultFlagHistory||"-99999"==payResultFlagCurrent){
			//禁用 同意  按钮
			forbiddenAndRelease(true);
		 	//进行代理商鉴权查询并获取大力商精细选择窗口
			validateAgent(idLoanOrder,payFlowKind);
		}else{
			if(confirm("您将花费"+amountRMB+"元购买"+flowDay+"个月的流水数据,请确认！")){
				//禁用 同意  按钮
				
				forbiddenAndRelease(true);
			 	//进行代理商鉴权查询并获取大力商精细选择窗口
				validateAgent(idLoanOrder,payFlowKind);
			}
		}
	}
 	
 	//ajax 进行代理商鉴权查询
 	function validateAgent(idLoanOrder,payFlowKind){
		var url = '/ifs-fsp-fe/validateAgent?payFlowKind='+payFlowKind;
		$.ajax({
 	 			type: "get",
  				url: url,
  				cache:false,
  				dataType:'json', 
  				success: function(data){   	
  					//解析结果：鉴权通过,打开选择窗口，进行选择付费
  					if("000000"==data.retcode){
  						openSelectMerchant(idLoanOrder);
  					}else{  						
  						forbiddenAndRelease(false);
  						$("#payAgreeButtonError").html(data.retmsg.split(",")[1]);
  					}
  					
			 },
			error:function(data){	
				alert("系统错误，请联系运维人员");
			}		
		});
 	}
 	//打开精细选择商户窗口
