 	//校验流水来源
	function changeflowSource(){
		$("#flowSourceError").html("");
		$("#flowSourceError").removeClass("err");
		$("#uploadFlow").attr("disabled",false);
		var flowSource = $("#flowSource").val();
		if(flowSource==''||flowSource==null){
			$("#flowSourceError").html("流水来源不能为空");
			$("#flowSourceError").addClass("err");
			$("#uploadFlow").attr("disabled",true);
			return false;
		}
		
		if(flowSource!=''){
		loadExcelFileName('36994',flowSource);
		$("#uploadFlowError").html("");
	  	$("#uploadFlowError").removeClass("err");
		}
		to_showSource();
		return true;
 	};
 	
