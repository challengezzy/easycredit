	//精确选择后进行购买
	//var index;
	function payFlowClick(){
		index=selectRadioValue();
		if(""==index){
			return;
		}
		var merchantMid=$("#merchantMid"+index).val();//商户识别码
		var merchantName=$("#merchantName"+index).val();//精确商户名称
		var payFlowKind=$("#payFlowKind").val();//流水期限
 		var idLoanOrder='36994';//订单id
 		queryBankData(merchantMid,merchantName,idLoanOrder,payFlowKind);
	}
	
	//ajax进行银联数据查询
	function  queryBankData(merchantMid,merchantName,idLoanOrder,payFlowKind){
		//TODO请求会乱码  uriEncode
		var url = '/ifs-fsp-fe/queryBankData';
		$.ajax({
 	 			type: "post",
  				url: url,
  				data:{merchantCode:merchantMid,merchantName:merchantName,idLoanOrder:idLoanOrder,payFlowKind:payFlowKind},
  				cache:false,
  				dataType:'json', 
  				success: function(data){  
  					payResultFlagCurrent=data.retcode 
  					//$("#payFlowClickError").html(data.retmsg);	
  					alert(data.retmsg);
			 },
			error:function(data){	
				alert("系统错误，请联系运维人员");
			}		
		});
	};
	//获取选择的详细商户信息
	function selectRadioValue(){
		var selectRadioValue=getRadioKey("payMerchantRadio");
		if(""==selectRadioValue){
			//$("#payFlowClickError").html("请选择一条商户信息");
			alert("请选择一条商户信息");
			return "";
		}
		return selectRadioValue;
	}
	
	//商户点击radio
	function checkValueEmpty(data){		
		$("tr[id^='selectTr']").each(function(index,item){			
			if(index==data){
				this.style.backgroundColor="#FFE4E1";
			}else{
				this.style.backgroundColor="#FFFFFF";
			}
		});		
	}
	//获取购买流水历史状态
	//var payFlowKindHistory;
	//var payResultFlagHistory;
	function  getPayFlagStatus(){
		var idLoanOrder='36994';
		var url = '/ifs-fsp-fe/queryPayFlagStatus?idLoanOrder='+idLoanOrder;
		$.ajax({
 	 			type: "get",
  				url: url,
  				cache:false,
  				dataType:'json', 
  				success: function(data){   	
  					payFlowKindHistory= data.payFlowKind;
  					payResultFlagHistory=data.payResultFlag;
			 },
			error:function(data){	
				alert("系统错误，请联系运维人员");
			}		
		});
	}

	//点击同意 延时较长  禁用放开按钮
	function  forbiddenAndRelease(flag){
		$("#payAgreeButton").attr("disabled",flag);
		$("#preStepButton").attr("disabled",flag);
		$("#saveButton").attr("disabled",flag);
		$("#nextStepButton").attr("disabled",flag);
	}
	//绑定事件
	function showSelectColor(){
		$("tr[id^='selectTr']").bind({
 			"mouseover":function(){
				var index=$(this).attr("id").replace("selectTr","");
				if(!$("#payMerchantRadio"+index).attr("checked")){						
					$(this).css("background-color","#E0FFFF");
				}
			},
			"mouseout":function(){
				var index=$(this).attr("id").replace("selectTr","");
				if(!$("#payMerchantRadio"+index).attr("checked")){						
					$(this).css("background-color","#FFFFFF");
				}
			}
 		});	
	};
</script>
