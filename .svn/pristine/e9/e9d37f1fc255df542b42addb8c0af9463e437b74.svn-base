 	function to_showSource(){
 		var flowSource = $("#flowSource").val();
 		if(flowSource=='699'){
			$("#flowSourceError").html("<a href='/ifs-fsp-fe/loadTemExcel/银行流水上传通用模板.xls'>通用模板下载</a>")
			$("#flowSourceError").addClass("loadHref");
 		}
