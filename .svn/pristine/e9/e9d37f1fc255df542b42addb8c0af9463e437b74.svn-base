  		}
  		clickFlag=true;
  		obj.close();
  };
	function check() {
		if(!clickFlag){
			alert("正在上传......");
			return false;
		}
		  var path =document.getElementById("myuploadExcel").value;
		  var start = path.lastIndexOf('.')+1;
		  var ext = path.substring(start,path.length).toLowerCase();
		  var array = ["xls"];
		  var b = false;
		  for(var i=0; i<array.length;i++) {
		      if(array[i]==$.trim(ext)) {
		         b = true;
		         break;
		      }
		  }
		  if(!b) {
			   alert("只能上传2003版本的excel");
			  return false;
		  }else {
		      return true;
		  }
	};
